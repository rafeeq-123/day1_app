<h1>Edit Workout Log!</h1>
<%= form_for(@workout, layout: :horizontal) do |f| %>
  <%= f.label :your_mood? %>
  <%= f.text_field :mood %>
  <%= f.label :workout %>
  <%= f.text_field :workout %>
  <%= f.label :how_long_was_your_workout? %>
  <%= f.text_field :length %>
  <%= f.label :date %>
  <%= f.date_select :date %>
  <%= f.submit%>
  <%=link_to "Cancel", root_path%>

 <%end%>